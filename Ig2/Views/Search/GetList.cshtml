@model Ig2.Models.PlainHolders.FoundItems
@using Ig2.Models.PlainHolders;
<div>
    <div>
        <div id="fwd">
        <button type="button" id="forward"></button>
         </div>
        <div>
            <table id="items" class="table table-striped">
                @foreach (ItemInfo item in Model.ItemList)
                {
                    <tr>
                        <td><img src="@item.img" /></td>
                        <td>@item.title</td>
                        <td class="cash">@item.price</td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>
<script src="~/Scripts/PartialFunc.js"></script>